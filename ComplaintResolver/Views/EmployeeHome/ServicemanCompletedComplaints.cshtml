@model IEnumerable<ComplaintResolver.Model.ComplaintForm>

@{
    ViewBag.Title = "ServicemanCompletedComplaints";
}

<h2>Completed Complaints</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Complaint_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date_assigned)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ComplaintDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReplyComments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employee_id)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Complaint_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date_assigned)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ComplaintDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReplyComments)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Employee_id)
            </td>

        </tr>
    }

</table>
